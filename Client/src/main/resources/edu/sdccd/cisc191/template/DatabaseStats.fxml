<AnchorPane prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.sdccd.cisc191.template.SceneControllers.DatabaseSceneController">
   <children>
      <Text layoutX="65.0" layoutY="120.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Global Database Stats">
         <font>
            <Font size="60.0" />
         </font>
      </Text>
      <GridPane fx:id="gridPane" layoutX="65.0" layoutY="136.0" prefHeight="747.0" prefWidth="894.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="894.0" minWidth="10.0" prefWidth="894.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="442.0" minWidth="0.0" prefWidth="0.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
