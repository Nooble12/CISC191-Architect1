          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="totalMoneyGained" text="Label">
               <font>
                  <Font size="50.0" />
               </font>
            </Label>
            <Label fx:id="totalMoneySpent" text="Label" GridPane.rowIndex="1">
